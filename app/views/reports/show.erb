
<div class="panel panel-default">
  <div class="panel-heading">
    <div class="title"> <h3 class="panel-title"><%= presenter.title %></h3></div>
    <div style="clear: both;"></div>
  </div>
  <table id="report" class="table table-striped table-hover">
    <thead>
      <th></th>
      <%= presenter.column_names %>
    </thead>
    <tbody>
      <%= presenter.rows %>
    </tbody>
  </table>
</div>

<form id="form-report-csv" class="form-horizontal" role="form" method="GET"
  action="reports/csv">
  <input type="hidden" value="<%= presenter.report.to_csv %>" name="csv_file_content" >
  <div class="form-group">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-default">Download csv file</button>
    </div>
  </div>
</form>

<form id="form-report-new" class="form-horizontal" role="form" method="GET"
  action="reports/new">
  <div class="form-group">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-default">Generate new report</button>
    </div>
  </div>
</form>